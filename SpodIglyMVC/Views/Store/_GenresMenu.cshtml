
@model IEnumerable<SpodIglyMVC.Models.Genre>



<nav class="genres-vertical">
    <ul>
        @foreach (var genre in Model)
            {
            <li>
                <a href="@Url.Action("List","Sotre", new { genrename= genre.Name.ToString()})" style="background-image: url(@Url.GenreIconPath(genre.IconFilename)); ">
                    @genre.Name
                </a>
            </li>
        }
        </ul>
</nav>
